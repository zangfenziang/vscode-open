{
	"name": "vscode-open-mini",
	"displayName": "open-mini",
	"description": "An extension for opening files",
	"version": "0.2.5",
	"publisher": "fxxzhang",
	"engines": {
		"vscode": "^1.43.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zangfenziang/vscode-open"
	},
	"homepage": "https://github.com/zangfenziang/vscode-open",
	"bugs": {
		"url": "https://github.com/zangfenziang/vscode-open/issues"
	},
	"categories": [
		"Other"
	],
	"icon": "images/icon.png",
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"keywords": [
		"open",
		"file",
		"browser"
	],
	"contributes": {
		"commands": [
			{
				"command": "vscode-open.open",
				"title": "Open: Open Current File"
			},
			{
				"command": "vscode-open.openPR",
				"title": "Open: Open Pull Request for Current Line"
			},
			{
				"command": "vscode-open.openLines",
				"title": "Open: Open Current File with Selected Lines"
			},
			{
				"command": "vscode-open.openURI",
				"title": "Open: Open File from URI"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "vscode-open.open",
					"group": "navigation"
				}
			]
		},
		"keybindings": [
			{
				"command": "vscode-open.open",
				"key": "ctrl+alt+o o",
				"mac": "cmd+alt+o o"
			}
		],
		"configuration": {
			"type": "object",
			"title": "Open",
			"properties": {
				"open.fileMappings": {
					"type": "array",
					"default": [],
					"description": "File mappings"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"lint": "eslint src --ext ts",
		"watch": "webpack --mode none --watch",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.43.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.0",
		"ts-loader": "^8.0.3",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0",
		"webpack": "^4.44.1",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"fp-ts": "^2.8.2",
		"io-ts": "^2.2.10",
		"simple-git": "^1.131.0",
		"yarn": "^1.22.4"
	}
}
